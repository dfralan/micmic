---
import CactusLost from "../components/blocks/CactusLost.astro";
import Container from "./blocks/Container.astro";
---

<Container
    customClass="flex flex-col w-full max-w-2xl h-screen items-center justify-center gap-2 text-center pt-24"
>
    <div class="max-w-screen w-xl h-fit p-6">
        <CactusLost />
    </div>

    <h1 class="text-2xl md:text-3xl w-full">Te perdiste?</h1>
    <p class="w-full">No te preocupes, te llevaremos al inicio ahora mismo.</p>
</Container>
