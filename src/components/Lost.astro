---
import CactusLost from "../components/blocks/CactusLost.astro";
import Container from "./blocks/Container.astro";
---

<Container>
    <div class="flex flex-col h-[350px] items-center justify-center">
        <div
            class="flex flex-row w-[200px] h-full items-center justify-center px-8"
        >
            <CactusLost />
        </div>

        <div
            class="flex flex-col w-full text-center items-center justify-center"
        >
            <h1 class="text-xl w-full">Te perdiste?</h1>
            <p class="w-full">
                No te preocupes, te llevaremos al inicio ahora mismo.
            </p>
        </div>
    </div>
</Container>
