---
import Layout from '../layouts/Layout.astro';
import DemoPreview from '../components/DemoPreview.astro';
import { SITE_TITLE, SITE_DESCRIPTION } from '../consts';

const meta = {
    "http-equiv": "refresh",
    content: "2000; url=/partner",
};
---

<Layout
	title={SITE_TITLE}
	description={SITE_DESCRIPTION}
	image='https://micmic.shop/img/link-img.jpg'
    meta={[meta]}
	hideHeader = {true}
	hideFooter = {true}
	>
	<main>
		<DemoPreview />
	</main>
</Layout>

