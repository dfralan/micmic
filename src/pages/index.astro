---
import Layout from '../layouts/Layout.astro';
import Load from '../components/Load.astro';
import { SITE_TITLE, SITE_DESCRIPTION } from '../consts';

const meta = {
    "http-equiv": "refresh",
    content: "2000; url=/partner",
};

const values = import.meta.env.some_value
---

<Layout
	title={SITE_TITLE}
	description={SITE_DESCRIPTION}
	image='https://micmic.shop/img/link-img.jpg'
    meta={[meta]}
	>
	<main>
		<Load />
		<p>{values}</p>

	</main>
</Layout>

